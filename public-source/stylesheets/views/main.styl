#content.main
  background #000
  position absolute
  top 0
  left 0
  right 0
  bottom 0

  #objectGrid
    position absolute
    top 88px
    bottom 0
    left 0
    right sidebarWidth
    background #000
    overflow hidden
    z-index 10

    .gridSlot
      position absolute
      width 12.5%
      height 12.5%
      box-shadow -1px 0 0 rgba(UIColor, 0.2) inset, 0 -1px 0 rgba(UIColor, 0.2) inset
      z-index 11
      &.drop-hover
        background rgba(UIColor, 0.1)
      // &.unavailable
      //   background blue
